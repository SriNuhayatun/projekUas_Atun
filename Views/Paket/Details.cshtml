@model List<Paket>
@{
    Layout = "_AdminLayout";
}
<div class="page-header">
    <h1 class="page-title">Details</h1>
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Home</a></li>
            <li class="breadcrumb-item active" aria-current="page"> Details Paket</li>
        </ol>
    </nav>
</div>
@foreach (var item in ViewBag.detail)
{
    <h3>Nama Paket  : @item.NamaPaket</h3>
    <h3>Lama Pinjam : @item.LamaPeminjaman </h3>
    
}

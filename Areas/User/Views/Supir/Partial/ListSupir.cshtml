@model SupirDashboard

<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <div class="table-responsive">
                <div class="card-header">
                </div>
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>ID Supir</th>
                            <th>Nama Supir</th>
                            <th>Alamat</th>
                            <th>Harga Perhari</th>
                            <th>status</th>
                            
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.supirr)
                        {
                            <tr>
                                <td>
                                    @item.Id_Supir
                                </td>
                                <td>
                                    @item.NamaSupir
                                </td>
                                <td>
                                    @item.Alamat
                                </td>
                                <td>
                                    @item.HargaPerhari
                                </td>
                                <td>
                                    @item.status
                                </td>
                            </tr>

                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>


@model List<Mobil>
@{
    Layout = "_AdminLayout";
}
<div class="page-header">
    <h1 class="page-title">Details</h1>
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Home</a></li>
            <li class="breadcrumb-item"><a asp-controller="Mobil" asp-action="Index">Data Mobil</a></li>
            <li class="breadcrumb-item active" aria-current="page"> Details Mobil</li>
        </ol>
    </nav>
</div>
@foreach (var item in ViewBag.detail)
{
    <h3>Nama Mobil : @item.NamaMobil</h3>
    <h3>Merk   : @item.Merk </h3>
    <h3>Status : @item.Status</h3>
    <h3>Harga  : @item.Harga</h3>
    <img src="@item.ImageMobil" width="100" height="120" />
}

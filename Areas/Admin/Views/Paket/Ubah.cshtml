@model Paket

@{
    Layout = "_AdminLayout";
}
<div class="page-header">
    <h1 class="page-title">Edit Paket</h1>
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Home</a></li>
            <li class="breadcrumb-item"><a asp-controller="Paket" asp-action="Index">Data Paket</a></li>
            <li class="breadcrumb-item active" aria-current="page"> Edit Paket</li>
        </ol>
    </nav>
</div>
<div class="row">
    <div class="col-md-6 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <form class="forms-sample" asp-controller="Paket" asp-action="Ubah">
                    <input type="hidden" asp-for="Id_Paket" class="form-control" id="exampleInputUsername1" />
                    <label>Nama Paket</label>
                    <input asp-for="NamaPaket" type="text" class="form-control" id="exampleInputUsername1" />
                    <label>Lama Peminjaman</label>
                    <input asp-for="LamaPeminjaman" class="form-control" id="exampleInputEmail1" />
                    <input type="submit" class="btn btn-primary mr-2" value="Submit" />
                </form>
            </div>
        </div>
    </div>
</div>